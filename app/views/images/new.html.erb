<h1 class="title" id="header">New Image</h1>
<%= form_with scope: :image, url: images_path, local: true do |form| %>
  <p>
    <%= form.label :url %><br>
    <%= form.text_field :url %><br><br>
    <%= form.label :tag_list, "Tags (separated by commas)" %><br>
    <%= form.text_field :tag_list %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
